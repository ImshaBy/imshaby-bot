{
  "name": "telegram-bot",
  "version": "0.2.0",
  "description": "Help volunteers to manage there parish",
  "main": "dist/index.js",
  "repository": "git@github.com:ImshaBy/imshaby-bot.git",
  "author": "Andrei Misan",
  "license": "MIT",
  "scripts": {
    "start": "npm run serve",
    "serve": "npm run build-ts && npm run start:prod",
    "serve-qa": "NODE_ENV=qa node dist/index.js",
    "watch-node": "nodemon dist/index.js",
    "eslint": "eslint -c .eslintrc.json .",
    "serve-debug": "nodemon --inspect dist/index.js",
    "start:prod": "node dist/index.js",
    "start:dev": "nodesrc/index.ts",
    "start:debug": "NODE_ENV=debug nodemon --inspect-rbk src/index.ts",
    "build": "npm run build-ts && npm run eslint",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "version-ts": "tsc -v",
    "debug": "npm run build && npm run watch-debug",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\"  \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "dependencies": {
    "@types/lodash": "^4.14.191",
    "axios": "^1.2.4",
    "body-parser": "^1.20.1",
    "dotenv": "^16.0.3",
    "express": "4.0.0",
    "express-jwt": "^8.3.0",
    "express-jwt-authz": "^2.4.1",
    "i18next": "^22.4.9",
    "i18next-fs-backend": "^2.1.1",
    "i18next-http-middleware": "^3.2.2",
    "ioredis": "^5.3.0",
    "jwks-rsa": "^3.0.1",
    "mongoose": "^6.9.0",
    "node-cron": "^3.0.2",
    "nodemon": "^2.0.22",
    "string-similarity": "^4.0.4",
    "telegraf": "3.38.0",
    "telegraf-i18n": "^6.6.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/node": "^18.11.18",
    "@types/node-cron": "^3.0.7",
    "@types/string-similarity": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.32.0",
    "eslint-plugin-jsdoc": "^39.6.8",
    "eslint-plugin-no-null": "^1.0.2"
  },
  "engines": {
    "node": "^16.20.0"
  }
}
